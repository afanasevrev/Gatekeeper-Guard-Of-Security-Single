         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="buttonDelete" onAction="#setButtonDelete" layoutX="40.0" layoutY="150.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="90.0" text="Удалить" />
      <Button fx:id="buttonCancel" onAction="#setButtonCancel" layoutX="160.0" layoutY="150.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="90.0" text="Отменить" />
   </children>
</AnchorPane>
